using System;
using System.Collections.Generic;
using System.Text;

namespace ReportFormDesign.Animals
{
    public abstract class AnimalInterpolation : Interpolation
    {

        public AnimalInterpolation()
        {
            Value = 1;
        }

        public float Value { get; set; }

        public float GetInterpolationValue()
        {
            return Value;
        }
    }
}
